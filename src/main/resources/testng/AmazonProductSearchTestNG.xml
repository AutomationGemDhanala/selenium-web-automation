<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite name="AmazonProductSearch">
    <test name="bluetooth" preserve-order="false">
        <classes>
            <class name="selenium.web.automation.testng.AmazonProductSearchTest">
                <methods>
                    <include name="validateAmazonProductSearchBluetooth"/>
                </methods>
            </class>
        </classes>
    </test>
    <test name="Led" preserve-order="false">
        <classes>
            <class name="selenium.web.automation.testng.AmazonProductSearchTest">
                <methods>
                    <include name="validateAmazonProductSearchLed"/>
                </methods>
            </class>
        </classes>
    </test>
    <test name="usb" preserve-order="false">
        <classes>
            <class name="selenium.web.automation.testng.AmazonProductSearchTest">
                <methods>
                    <include name="validateAmazonProductSearchUsb"/>
                </methods>
            </class>
        </classes>
    </test>
    <test name="watch" preserve-order="false">
        <classes>
            <class name="selenium.web.automation.testng.AmazonProductSearchTest">
                <methods>
                    <include name="validateAmazonProductSearchWatch"/>
                </methods>
            </class>
        </classes>
    </test>
</suite>
